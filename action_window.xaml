<Window x:Class="hotehet.action_window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:hotehet"
        mc:Ignorable="d"
        Title="action_window" Height="450" Width="800">
    <Grid Background="Black">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="3*" />
        </Grid.ColumnDefinitions>
        <Frame Name="menu_frame" NavigationUIVisibility="hidden" />
        <Frame Grid.Column="1" Name="main_frame" NavigationUIVisibility="hidden"/>
    </Grid>
</Window>
